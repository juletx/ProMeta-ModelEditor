  <sections xmi:id="_etwusZ01EdyQ3oTO93enUw" name="The implementation conforms to the architecture and design" guid="_etwusZ01EdyQ3oTO93enUw" variabilityBasedOnElement="_etwusZ01EdyQ3oTO93enUw" sectionDescription="&lt;ul>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Is the implementation structured as specified in the design?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Are all of the functions in the design implemented?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Are all of the interfaces in the design implemented according to their specifications?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Does the implementation adhere to all design and architectural constraints?&#xD;&#xA;    &lt;/li>&#xD;&#xA;&lt;/ul>"/>
  <sections xmi:id="_etwutJ01EdyQ3oTO93enUw" name="The implementation is testable" guid="_etwutJ01EdyQ3oTO93enUw" variabilityBasedOnElement="_etwutJ01EdyQ3oTO93enUw" sectionDescription="&lt;ul>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Can you test the expected behavior at the unit level?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Is the code written in a way that all paths can be exercised?&#xD;&#xA;    &lt;/li>&#xD;&#xA;&lt;/ul>"/>
  <sections xmi:id="_etwutZ01EdyQ3oTO93enUw" name="The implementation is correct" guid="_etwutZ01EdyQ3oTO93enUw" variabilityBasedOnElement="_etwutZ01EdyQ3oTO93enUw" sectionDescription="&lt;ul>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Does the implementation pass all of the developer tests?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Does the implementation support the acceptance criteria of the test cases?&#xD;&#xA;    &lt;/li>&#xD;&#xA;    &lt;li>&#xD;&#xA;        Is all code executable (no dead code areas)?&#xD;&#xA;    &lt;/li>&#xD;&#xA;&lt;/ul>"/>
