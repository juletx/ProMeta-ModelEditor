          <BreakdownElement xsi:type="org.eclipse.epf.uma:TaskDescriptor" name="package_the_release" briefDescription="Each release should be built and packaged in a standard, controlled, and repeatable manner." guid="_y7a0GJiLEeGOvpP1fVrVNA" orderingGuide="" presentationName="Package the Release" suppressed="false" isAbstract="false" hasMultipleOccurrences="false" isOptional="false" isPlanned="true" prefix="" isEventDriven="false" isOngoing="false" isRepeatable="false" isSynchronizedWithSource="true">
