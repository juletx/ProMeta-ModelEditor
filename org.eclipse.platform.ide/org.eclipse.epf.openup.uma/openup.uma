    <methodPackages xsi:type="org.eclipse.epf.uma:ContentPackage" name="Management Work Product Slots" briefDescription="" guid="_xo1MYEfUEdyiPI8btkmvmw" orderingGuide="" presentationName="" suppressed="false" global="false">
      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:Artifact" name="project_definition_and_scope_slot" briefDescription="This slot serves as an abstraction of high-level artifacts that define the project and its scope. Typical examples of such artifacts could be a project definition, and a high-level project schedule identifying major milestones and major deliverables." guid="_RJMUcNpUEdyzZqGyZ7hwdw" orderingGuide="" presentationName="[Project Definition and Scope]" suppressed="false" isAbstract="true" variabilityType="na"/>
      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:Artifact" name="project_status_slot" briefDescription="This slot serves as an abstraction for any type of status report or assessment of the project's progress. This includes both internal assessments, as well as those of interest to the project sponsors." guid="_kf7foNpUEdyzZqGyZ7hwdw" orderingGuide="" presentationName="[Project Status]" suppressed="false" isAbstract="true" variabilityType="na"/>
      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:Artifact" name="project_risk_slot" briefDescription="This slot serves as an abstraction for artifacts that list, track, and manage project risk, such as a risk list or risk log." guid="_0TkXgNpUEdyzZqGyZ7hwdw" orderingGuide="" presentationName="[Project Risk]" suppressed="false" isAbstract="true" variabilityType="na"/>
