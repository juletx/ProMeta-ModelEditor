    <methodPackages xsi:type="org.eclipse.epf.uma:ProcessPackage" name="Risk-Value Lifecycle Phases" briefDescription="" guid="_LYTG0OL6EdyM47cGD2jiaQ" orderingGuide="" presentationName="" suppressed="false" global="false">
