      <MethodPackage xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:ProcessComponent" name="prepare_for_release" briefDescription="" guid="_y7az-ZiLEeGOvpP1fVrVNA" orderingGuide="" presentationName="" suppressed="false" global="false" authors="" changeDescription="" version="">
