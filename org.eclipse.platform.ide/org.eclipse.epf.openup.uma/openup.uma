        <Presentation xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:TaskDescription" name=",_XlJ80CZpEd-XKYR2S73PfQ" briefDescription="" guid="-dWzB4mHRr5uSP98BqAOy1g" orderingGuide="" presentationName="" suppressed="false" authors="" changeDate="2010-03-10T10:44:03" changeDescription="" version="7.5.0" externalId="">
