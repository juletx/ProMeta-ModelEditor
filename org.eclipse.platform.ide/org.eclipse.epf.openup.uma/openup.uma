  <methodPlugins copyrightStatement="_uuunoPsDEdmyhNQr5STrZQ" bases="_iVydgENdEdy245g62lfWcA _-bv0ELXuEduGP_98XMd0fg _OOB2AAouEdyhlpBPrduOMw _vsno4EKmEdyrauqpmnA8jQ _0TmecJ-VEdy3Nc4rvuj7bA" name="practice.tech.continuous_integration.base" briefDescription="Continuously maintain an integrated build and evolve it through frequent delivery of changes." guid="_D-ExQMktEdygFZkm1d5NtQ" orderingGuide="" presentationName="Continuous Integration" suppressed="false" authors="" changeDate="2008-09-22T06:43:27" changeDescription="" version="" supporting="false" userChangeable="true">
