      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:TermDefinition" name="stakeholder_need" briefDescription="" guid="_WUiFcAeYEduWycDgioo5rg" orderingGuide="" presentationName="stakeholder need" suppressed="false" isAbstract="false" variabilityType="na">
        <Presentation xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" name="new_term_definition,_WUiFcAeYEduWycDgioo5rg" briefDescription="" guid="-1pmL5bC27rtWB84PXAgq9Q" orderingGuide="" presentationName="" suppressed="false" authors="" changeDescription="" version="7.1.0" externalId="">
          <MainDescription>The business or operational problem (opportunity) that must be fulfilled to justify purchase or use of the system.</MainDescription>
          <KeyConsiderations/>
        </Presentation>
