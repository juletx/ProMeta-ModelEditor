        <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:Template" name="project_plan" briefDescription="This is the informal template for representing the project plan." guid="_0c7hoMlgEdmt3adZL5Dmdw" orderingGuide="" presentationName="Project Plan" suppressed="false" isAbstract="false" variabilityType="na">
          <Presentation xsi:type="org.eclipse.epf.uma:GuidanceDescription" name="project_plan,_0c7hoMlgEdmt3adZL5Dmdw" briefDescription="" guid="_XjOXcMM2EdmSIPI87WLu3g" orderingGuide="" presentationName="" suppressed="false" authors="" changeDate="2006-05-08T02:03:04" changeDescription="" version="7.5.1" externalId="">
