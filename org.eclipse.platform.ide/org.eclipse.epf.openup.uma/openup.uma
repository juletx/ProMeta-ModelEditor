  <methodPlugins copyrightStatement="_uuunoPsDEdmyhNQr5STrZQ" bases="_ji3-gEMXEdyP_Jn83ylwUQ _OOB2AAouEdyhlpBPrduOMw _iVydgENdEdy245g62lfWcA _-bv0ELXuEduGP_98XMd0fg _vsno4EKmEdyrauqpmnA8jQ" name="practice.tech.concurrent_testing.base" briefDescription="Focus the testing effort where it will provide the greatest value, by applying agile testing principles." guid="_Hz9OMG5KEdyAHIxjZ9kpqw" orderingGuide="" presentationName="Concurrent Testing" suppressed="false" authors="" changeDate="2008-07-21T10:21:52" changeDescription="" version="" supporting="false" userChangeable="true">
