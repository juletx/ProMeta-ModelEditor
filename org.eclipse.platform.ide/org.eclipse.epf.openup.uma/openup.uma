      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:TermDefinition" name="iteration" briefDescription="" guid="_5vWoEKNcEdyMFYhoCpD11Q" orderingGuide="" presentationName="iteration" suppressed="false" isAbstract="false" variabilityType="na">
