    <methodPackages xsi:type="org.eclipse.epf.uma:ContentCategoryPackage" name="ContentCategories" guid="_brFioWsbEeuUEbajkWrRrg">
      <ContentCategory xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:Tool" name="cvs_tool" briefDescription="" guid="_hFih0MYbEdyXeo04os5BOw" orderingGuide="" presentationName="CVS" suppressed="false" isAbstract="false" variabilityType="na">
