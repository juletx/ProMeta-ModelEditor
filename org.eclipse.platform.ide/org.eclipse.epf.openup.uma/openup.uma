    <methodPackages xsi:type="org.eclipse.epf.uma:ContentPackage" name="production_release" briefDescription="" guid="_IADPYOB-EeC1y_NExchKwQ" orderingGuide="" presentationName="Production Release" suppressed="false" global="false">
      <MethodPackage xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:ContentPackage" name="release_preparation" briefDescription="" guid="_IAHg2OB-EeC1y_NExchKwQ" orderingGuide="" presentationName="Release Preparation" suppressed="false" global="false">
