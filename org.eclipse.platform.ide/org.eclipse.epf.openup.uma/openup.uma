      <MethodPackage xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:ContentPackage" name="template_report_and_examples" briefDescription="" guid="_B3azkMh5EdygFZkm1d5NtQ" orderingGuide="" presentationName="Template, Report, and Examples" suppressed="false" global="false">
