          <BreakdownElement xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" name="project_work_slot" briefDescription="This slot serves as an abstraction for any type of work being done on the project. It could be represented as a work items list, an operational schedule, a work breakdown structure, and so on." guid="_wU5YkG7rEd2yFK8n6tDvyw" orderingGuide="" presentationName="[Project Work]" suppressed="false" isAbstract="false" hasMultipleOccurrences="false" isOptional="false" isPlanned="false" prefix="" isSynchronizedWithSource="true" activityEntryState="" activityExitState="">
