            <Presentation xsi:type="org.eclipse.epf.uma:DescriptorDescription" name="02_01_package_the_release,_EMH-APd4EeCFmpfCmMPILg" briefDescription="" guid="-zxy4hzFT2yBzt2lUjCwnqA" orderingGuide="" presentationName="" suppressed="false" authors="" changeDescription="" version="" externalId="" usageGuidance="">
