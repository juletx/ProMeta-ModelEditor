          <BreakdownElement xsi:type="org.eclipse.epf.uma:Activity" name="plan_manage_iteration" briefDescription="" guid="_3s7aYdONEdyqlogshP8l4g" orderingGuide="" presentationName="" suppressed="false" isAbstract="false" hasMultipleOccurrences="false" isOptional="false" isPlanned="true" prefix="" isEventDriven="false" isOngoing="false" isRepeatable="false" variabilityBasedOnElement="_oZgCsdOEEdyqlogshP8l4g" variabilityType="extends">
