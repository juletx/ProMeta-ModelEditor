      <contentElements supportingMaterials="" conceptsAndPapers="" checklists="" guidelines="" examples="" assets="" termdefinition="" xsi:type="org.eclipse.epf.uma:Artifact" name="tools" briefDescription="These work products are the tools needed to support the software development effort. " guid="_AeUr0EmiEd-UYMQAaXd4fQ" orderingGuide="" presentationName="Tools" suppressed="false" isAbstract="false" variabilityType="na">
        <Presentation xmlns:uma="http://www.eclipse.org/epf/uma/1.0.6" xsi:type="org.eclipse.epf.uma:ArtifactDescription" name="new_artifact,_AeUr0EmiEd-UYMQAaXd4fQ" briefDescription="" guid="-7TKJK1WF21Hi03R3UAjexQ" orderingGuide="" presentationName="" suppressed="false" authors="" changeDate="2010-04-16T11:50:52" changeDescription="" version="7.5.0" externalId="">
